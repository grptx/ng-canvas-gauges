{
  "name": "@mousazadeh/ng-canvas-gauges",
  "version": "13.0.0",
  "description": "Angular 13+ component wrapper for the canvas-gauges lib written by @Mikhus",
  "scripts": {
    "build:example": "ng build example",
    "prebuild:lib": "npm run clean",
    "build:lib": "ng build ng-canvas-gauges",
    "postbuild:lib": "cp -f ./LICENSE  ./README.md ./gauges.png ./gauges.gif ./dist/ng-canvas-gauges",
    "build:pack": "cd dist/ng-canvas-gauges && npm pack",
    "build:package": "npm run build:lib && npm run build:pack",
    "postbuild:package": "mkdir lib && cp dist/ng-canvas-gauges/mousazadeh-ng-canvas-gauges-*.tgz lib",
    "clean": "rm -rf dist/ lib/",
    "start": "ng serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mousazadeh/ng-canvas-gauges.git"
  },
  "keywords": [
    "Angular",
    "Angular 13",
    "Canvas",
    "Gauges",
    "Thermometer",
    "Meter",
    "Speedometer"
  ],
  "author": "al.mousazadeh@gmail.com",
  "contributors": [
    "Alireza Mousazadeh"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MeetmeLeave/ng-canvas-gauges/issues"
  },
  "homepage": "https://github.com/MeetmeLeave/ng-canvas-gauges",
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@types/canvas-gauges": "^2.1.2",
    "canvas-gauges": "^2.1.7",
    "core-js": "^3.6.5",
    "rx-dom-html": "^7.0.3",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.8",
    "@angular/cli": "~13.3.8",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "^13.3.8",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.10.0",
    "codelyzer": "^6.0.2",
    "ng-packagr": "^13.3.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~4.6.2",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0"
  },
  "main": "index.js",
  "directories": {
    "lib": "lib"
  }
}
