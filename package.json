{
  "name": "@mousazadeh/ng-canvas-gauges",
  "version": "14.0.7",
  "description": "Angular 14+ component wrapper for the canvas-gauges lib written by @Mikhus",
  "scripts": {
    "build:example": "ng build example",
    "prebuild:lib": "npm run clean",
    "build:lib": "ng build ng-canvas-gauges",
    "postbuild:lib": "cp -f ./LICENSE  ./README.md ./gauges.png ./gauges.gif ./dist/ng-canvas-gauges",
    "build:pack": "cd dist/ng-canvas-gauges && npm pack",
    "build:package": "npm run build:lib && npm run build:pack",
    "postbuild:package": "mkdir lib && cp dist/ng-canvas-gauges/mousazadeh-ng-canvas-gauges-*.tgz lib",
    "publish": "npm publish --access public",
    "clean": "rm -rf dist/ lib/",
    "start": "ng serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mousazadeh/ng-canvas-gauges.git"
  },
  "keywords": [
    "Angular",
    "Angular 13",
    "Angular 14",
    "Canvas",
    "Gauge",
    "Gauges",
    "Thermometer",
    "Meter",
    "Speedometer",
    "Angular 13 gauge",
    "Angular 14 gauge",
    "Angular 13 gauges",
    "Angular 14 gauges"
  ],
  "author": "al.mousazadeh@gmail.com",
  "contributors": [
    "Alireza Mousazadeh"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mousazadeh/ng-canvas-gauges/issues"
  },
  "homepage": "https://github.com/mousazadeh/ng-canvas-gauges",
  "dependencies": {
    "@angular/animations": "~14.0.5",
    "@angular/common": "~14.0.5",
    "@angular/compiler": "~14.0.5",
    "@angular/core": "~14.0.5",
    "@angular/forms": "~14.0.5",
    "@angular/platform-browser": "~14.0.5",
    "@angular/platform-browser-dynamic": "~14.0.5",
    "@angular/router": "~14.0.5",
    "@types/canvas-gauges": "^2.1.4",
    "canvas-gauges": "^2.1.7",
    "core-js": "^3.23.4",
    "rx-dom-html": "^7.0.3",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.0.5",
    "@angular/cli": "~14.0.5",
    "@angular/compiler-cli": "~14.0.5",
    "@angular/language-service": "^14.0.5",
    "@types/node": "^18.0.3",
    "@types/jasmine": "~4.0.3",
    "codelyzer": "^6.0.2",
    "ng-packagr": "^14.0.3",
    "ts-node": "~10.8.2",
    "tslint": "~5.20.1",
    "typescript": "~4.7.4",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0"
  },
  "main": "index.js",
  "directories": {
    "lib": "lib"
  }
}
